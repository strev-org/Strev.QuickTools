  <files>
    <file src="{package_path}\Release\{target1}\{package_name}.dll" target="lib\{target1}\{package_name}.dll" />
    <file src="{package_path}\Release\{target2}\{package_name}.dll" target="lib\{target2}\{package_name}.dll" />
    <file src="{package_path}\..\..\res\QuickTools-32.png" target="images\" />
  </files> 
